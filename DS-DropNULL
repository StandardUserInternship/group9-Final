import pandas as pd
import numpy as np
import matplotlib.pyplot as plt

df = pd.read_csv("train.csv")
df.head()

pct_null = df.isnull().sum() / len(df)
missing_features = pct_null[pct_null > 0.80].index
df1 = df.drop(missing_features, axis=1, inplace=True)

