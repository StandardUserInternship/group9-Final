import pandas as pd
import numpy as np
import matplotlib.pyplot as plt

def Clean_data():
    df = pd.read_csv("train.csv")
    for column in Clean_data.columns:
        numNull = Clean_data[column].isnull().sum()
        twntypercnt = (Clean_data.shape[0]* 0.20)
        if numNull > twntypercnt:
            Clean_data = Clean_data.drop(column, axis = 1)
            continue
    return Clean_data




firstOption= input("type only one: Sex, Age, or Survived: ")
secondOption = input("Type ony one from: PassengerId, Embarked, Cabin, or Pclass: ")

#chosenData = Clean_data.groupby(firstOption)[secondOption].mean
print(Clean_data)
plt.scatter('firstOption','secondOption')
plt.show

